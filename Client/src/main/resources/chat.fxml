   <Label fx:id="pathLabelServer" layoutX="435.0" layoutY="40.0" prefHeight="25.0" prefWidth="246.0" text="Label" />
   <Button fx:id="buttonUpServer" layoutX="680.0" layoutY="38.0" mnemonicParsing="false" onAction="#serverPathUp" prefHeight="25.0" prefWidth="57.0" text="Наверх" />
