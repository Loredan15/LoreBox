<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="550.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
    <ListView fx:id="clientView" layoutX="14.0" layoutY="67.0" prefHeight="425.0" prefWidth="301.0" />
   <Label fx:id="pathLabelClient" layoutX="14.0" layoutY="41.0" prefHeight="25.0" prefWidth="246.0" text="Label" />
   <Button fx:id="buttonUpClient" layoutX="259.0" layoutY="39.0" mnemonicParsing="false" onAction="#clientPathUp" prefHeight="25.0" prefWidth="57.0" text="Наверх" />
   <ListView fx:id="serverView" layoutX="435.0" layoutY="67.0" prefHeight="425.0" prefWidth="301.0" />
   <Label alignment="CENTER" contentDisplay="CENTER" layoutX="100.0" layoutY="12.0" prefHeight="17.0" prefWidth="130.0" text="Client">
      <font>
         <Font size="14.0" />
      </font></Label>
